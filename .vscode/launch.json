{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/sdl game",
      "program": "c:/sdl game/main.exe",
      "environment": [],
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "miDebuggerPath": "C:/msys64/ucrt64/bin/gdb.exe",
      "preLaunchTask": "build",
      "miDebuggerArgs": "",
      "logging": {
          "engineLogging": true
      },
      "cppStandard": "c++17",
      "intelliSenseMode": "windows-gcc-x86",
      "browse": {
          "path": [
              "C:/sdl game/src/include",
              "C:/sdl game/src/include",
              "C:/sdl game/src/include"
          ],
          "limitSymbolsToIncludedHeaders": true
      }
    }
  ]
}